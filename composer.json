{
    "name": "vicenterusso/laravel-facilitamovel-channel",
    "description": "Facilita Movel notification channel for Laravel",
    "homepage": "https://github.com/vicenterusso/laravel-facilitamovel-channel",
    "license": "MIT",
    "authors": [
        {
            "name": "Vicente Russo Neto",
            "email": "vicente.russo@gmail.com",
            "homepage": "http://www.vrusso.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0",
        "illuminate/support": "~6.0 || ~7.0 || ~8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\FacilitaMovel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\FacilitaMovel\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\FacilitaMovel\\FacilitaMovelServiceProvider"
            ]
        }
    }
}
